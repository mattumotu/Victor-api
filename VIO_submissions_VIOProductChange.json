                "parameters": [
                    {
                        "in": "query",
                        "name": "vioProductChangeRequest",
                        "description": "",
                        "required": True,
                        "type": "VIOProductChangeRequest"
                    },
                ],
                "responses": {
                    "200": {
                        "description": "",
                        "schema": {
                            "$ref": "#/definitions/VIOProductChangeResponse"
                        }
                    },
                    "400": {
                        "description": "Bad Request, see returned error message for details."
                    }
                }
            }
        }
    },
